public class Cartao{
  private int id;
  private String cpfPessoa;
  private int idadePessoa;
  private String validadecartao;
  private String nomePessoa;
  private boolean pagoucartao;
  
}

public Cartao(int id, String cpfPessoa, int idadePessoa, String validadecartao, String nomePessoa, boolean pagoucartao){
  this.id = id;
  this.cpfPessoa = cpfPessoa;
  this.idadePessoa = idadePessoa;
  this.validadecartao = validadecartao;
  this.nomePessoa = nomePessoa;
  this.pagoucartao = pagoucartao;
  }

public boolean pagoucartao(){
  return true;
}

